{
  "name": "quote-calc-system-api",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "prebuild": "npm run clean && npm run lint:fix",
    "build": "tsc",
    "clean": "rimraf dist",
    "clean:codegen": "rimraf -g -v src/apis/index.ts src/apis/*/{index,types}.ts src/models/*.ts",
    "precodegen": "npm run openapi && npm run clean:codegen",
    "codegen": "openapi-generator-cli generate -c openapi/config.yml",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "openapi": "redocly bundle -d --remove-unused-components -o openapi/openapi.yml ../../docs/openapi/index.yml",
    "postinstall": "npm -C node_modules/oracledb run prune all",
    "start": "nodemon -e ts,json,yml,yaml -x \"ts-node -r dotenv/config -r tsconfig-paths/register bin/www\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@openapitools/openapi-generator-cli": "^2.24.0",
    "@redocly/cli": "^2.2.2",
    "@types/aws-lambda": "^8.10.152",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/http-errors": "^2.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.6.0",
    "@types/nodemailer": "^7.0.2",
    "@types/oracledb": "^6.9.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.2"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.27.0",
    "@aws-sdk/client-secrets-manager": "^3.899.0",
    "@codegenie/serverless-express": "^4.17.0",
    "@middy/core": "^6.4.5",
    "axios": "^1.12.2",
    "cookie-parser": "^1.4.7",
    "csv-stringify": "^6.6.0",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-openapi-validator": "^5.6.0",
    "format-message": "^6.2.4",
    "http-errors": "^2.0.0",
    "i18next": "^25.5.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nodemailer": "^7.0.6",
    "oracledb": "^6.9.0"
  }
}
